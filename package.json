{
  "name": "goldlight-identifier",
  "displayName": "Goldlight.Identifier",
  "description": "Generate UUIDs inside your code editor",
  "version": "0.0.1",
  "img": "https://github.com/ohanlon/goldlight-identifier/blob/main/src/goldlight.png",
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "goldlight-identifier.createIdentifier",
        "title": "Generate UUID"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "goldlight-identifier.createIdentifier",
          "when": "editorFocus && !editorReadonly"
        }
      ]
    },
    "keybindings": [
      {
        "command": "goldlight-identifier.createIdentifier",
        "key": "ctrl+u ctrl+u",
        "mac": "cmd+u cmd+u",
        "when": "editorTextFocus && !editorReadonly"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.76.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.34.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5",
    "@vscode/test-electron": "^2.2.3"
  },
  "dependencies": {
    "uuid": "^9.0.0"
  }
}
